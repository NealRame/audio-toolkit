{
	"folders":
	[
		{
			"path": "@CMAKE_SOURCE_DIR@",
			"follow_symlinks": true,
			"file_exclude_patterns": [ "Makefile.depends", "@CMAKE_PROJECT_NAME@.sublime-project" ]
		}
	],
	"settings":
	{
		"tab_size": 8,
		"translate_tabs_to_spaces": false,
		"sublimeclang_options": [
			"-ferror-limit=0",
			"-I/Library/Developer/CommandLineTools/usr/lib/c++/v1/",
			"-I/usr/include/x86_64-linux-gnu/c++/4.8",
			@INCLUDE_DIRECTORIES@
		],
		"sublimeclang_additional_language_options": {
			"c++": [ @CXX_FLAGS@ ]
		}
	},
	"build_systems":
	[
		{
			"name": "@CMAKE_PROJECT_NAME@",
			"cmd": [ "make", "-j", "-C", "@CMAKE_BINARY_DIR@" ]
		}
	]
}
